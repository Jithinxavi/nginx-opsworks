server {
  listen   80;
  server_name  127.0.0.1;

  access_log  <%= node[:nginx][:log_dir] %>/localhost.access.log;

  location / {
    root   /var/www/nginx-default;
    index  index.html index.htm;
  }
  
        location /static {
                proxy_pass http://54.226.109.157;
        }
        location ~ \.php$ {
               proxy_pass http://174.129.163.35;
        }
}
